generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User{
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  name      String?
}

model Lost{
  id        Int     @id @default(autoincrement())
}

model Found{
  id        Int     @id @default(autoincrement())
}

model subjectAttendace {
  id        Int     @id @default(autoincrement())
}
